{
   "AWSEBDockerrunVersion": 1,
   "containerDefinitions": [
     {
       "name": "ui",
       "image": "{$AWS_ACCOUNT_ID}.dkr.ecr.{$AWS_DEFAULT_REGION}.amazonaws.com/app_ui",
       "hostname": "ui",
       "essential": true,
       "memory": 1024,
       "portMappings": [
         {
           "hostPort": 4200,
           "containerPort": 4200
         }
       ]
     },
     {
       "name": "api",
       "image": "{$AWS_ACCOUNT_ID}.dkr.ecr.{$AWS_DEFAULT_REGION}.amazonaws.com/app_api",
       "hostname": "api",
       "essential": true,
       "memory": 512,
       "portMappings": [
         {
           "hostPort": 5000,
           "containerPort": 5000
         }
       ]
     }
   ]
 }