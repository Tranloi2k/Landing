{
   "AWSEBDockerrunVersion": 2,
   "containerDefinitions": [
     {
       "name": "ui",
       "image": "public.ecr.aws/j5g4t9t6/angular_ui",
       "hostname": "ui",
       "essential": true,
       "memory": 1024,
       "portMappings": [
         {
           "hostPort": 4200,
           "containerPort": 4200
         }
       ]
     },
     {
       "name": "api",
       "image": "public.ecr.aws/j5g4t9t6/python_api",
       "hostname": "api",
       "essential": true,
       "memory": 512,
       "portMappings": [
         {
           "hostPort": 3000,
           "containerPort": 3000
         }
       ]
     }
   ]
 }